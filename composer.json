{
    "name": "thebachtiarz-company/laravel-whatsapp-v1",
    "description": "A Library for whatsapp messenger application v1",
    "type": "library",
    "version": "1.0.3",
    "require": {
        "php": "^8.1",
        "laravel/framework": "^10.18",
        "thebachtiarz-company/laravel-base-v1": "^1.0"
    },
    "require-dev": {
        "doctrine/coding-standard": "^12.0"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "TheBachtiarz\\WhatsApp\\": "src/"
        },
        "files": [
            "helper/helper.php"
        ]
    },
    "authors": [
        {
            "name": "thebachtiarz-company",
            "email": "thebachtiarz.company@gmail.com"
        }
    ],
    "keywords": [
        "laravel",
        "thebachtiarz",
        "whatsapp"
    ],
    "extra": {
        "laravel": {
            "providers": [
                "TheBachtiarz\\WhatsApp\\ServiceProvider"
            ]
        }
    },
    "homepage": "https://github.com/thebachtiarz-company/laravel-whatsapp-v1",
    "support": {
        "issues": "https://github.com/thebachtiarz-company/laravel-whatsapp-v1/issues",
        "source": "https://github.com/thebachtiarz-company/laravel-whatsapp-v1"
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
